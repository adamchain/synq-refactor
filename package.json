{
  "name": "synq",
  "private": true,
  "version": "1.0.0",
  "description": "SYNQ Express Server & React App",
  "scripts": {
    "test-app": "concurrently -n=\"server,client\" -c=\"green,cyan\"  \"npm:start:server:test\" \"npm:start:client:dev\"",
    "start:server:test": "cross-env NODE_ENV=test nodemon --ignore 'src' --ignore 'cypress' --exec \"node server/index.mjs\"",
    "start:server:test:ci": "cross-env NODE_ENV=test node server/index.mjs",
    "start:client:dev": "cross-env NODE_ENV=development webpack serve --port 3000",
    "dev": "cross-env NODE_ENV=dev DEV=1 nodemon --ignore 'src' --ignore 'cypress' --exec \"node server/index.mjs\"",
    "local": "cross-env NODE_ENV=local nodemon --ignore 'src' --ignore 'cypress' --exec \"node server/index.mjs\"",
    "watch": "webpack --watch",
    "build": "cross-env GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "lint": "eslint ./src --ext .jsx --ext .js",
    "lint:fix": "eslint ./src --ext .jsx --ext .js --fix",
    "test": "cross-env NODE_ENV=test jest --runInBand --forceExit",
    "prepare": "husky install",
    "test:watch": "jest --watch **/*.spec.js",
    "test:coverage": "jest --coverage **/*.spec.js",
    "migrations:test": "cross-env npx @databases/pg-migrations apply --directory migrations",
    "format": "npx prettier --write .",
    "cypress:open": "cypress open",
    "start": "node server/index.mjs",
    "web": "react-scripts start",
    "server": "npm run start:server:test",
    "build:schema": "sequelize-auto -h synq-db-uat.c9s22uqoi5rc.us-east-2.rds.amazonaws.com -d whskr -u admin -x -p 3306 --dialect mysql -o ./server/models --cm c -l ts"
  },
  "main": "index.js",
  "engines": {
    "node": "^20.18.3",
    "npm": "^10.8.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [],
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WHSKRECHNOLOGIES/snyq.git"
